USE DATABASE UkPostcodes;
USE SCHEMA Postcodes;
  
CREATE TABLE IF NOT EXISTS Postcodes_DistrictCode
(
 Postcode string,
 CountyCode string,
 DistrictCode string,
 CountryCode string,
 Latitude decimal?,
 Longitude decimal?,
 INDEX idx_Postcodes
 CLUSTERED(Postcode)
 DISTRIBUTED BY HASH (DistrictCode)
);